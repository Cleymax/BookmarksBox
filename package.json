{
  "name": "bookmarksbox",
  "version": "1.0.0",
  "description": "Team Bookmarking",
  "scripts": {
    "dev-server": "concurrently \"yarn watch\" \"yarn live-server\"",
    "watch-sass": "node-sass --output-style expanded --source-map true -w -r resources/scss -o public/css",
    "server": "live-server --host=0.0.0.0 --ignore=package.json",
    "build-sass": "node-sass --output-style compressed -x -r resources/scss -o public/css",
    "build": "webpack --mode=production",
    "dev": "cross-env NODE_ENV=dev webpack --mode=development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cleymax/BookmarksBox.git"
  },
  "keywords": [
    "bookmark",
    "box",
    "php"
  ],
  "author": "Clement PERRIN <clement.perrin@etu.univ-smb.fr>",
  "bugs": {
    "url": "https://github.com/Cleymax/BookmarksBox/issues"
  },
  "homepage": "https://github.com/Cleymax/BookmarksBox#readme",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^1.5.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.10",
    "postcss-loader": "^5.2.0",
    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  }
}
